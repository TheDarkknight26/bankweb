{
    "version": 2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" },
      { "src": "/api/result/.*", "dest": "index.js" },
      { "src": "/api/resultbtwdates/.*", "dest": "index.js" },
      { "src":"/api/fdrates/.*","dest":"index.js"}
     
    ]
  }
